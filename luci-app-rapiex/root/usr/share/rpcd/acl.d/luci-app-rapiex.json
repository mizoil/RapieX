{
  "luci-app-rapiex": {
    "description": "Grant UCI and RPC access to LuCI app rapiex",
    "read": {
      "file": {
        "/etc/init.d/rapiex": [
          "exec"
        ]
      },
      "ubus": {
        "service": [
          "list"
        ]
      },
      "uci": [
        "rapiex"
      ]
    },
    "write": {
      "uci": [
        "rapiex"
      ]
    }
  }
}